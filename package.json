{
  "name": "fastify-starter",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start-dev": "nodemon -L -e ejs,js src/index.js",
    "fix": "npm run prettier",
    "test": "jest"
  },
  "engines": {
    "node": "12.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "pre-push": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node"
  },
  "author": "Jake Fournier",
  "license": "UNLICENSED",
  "description": "",
  "dependencies": {
    "ejs": "^3.1.2",
    "fastify": "^2.14.0",
    "fastify-cookie": "^3.6.1",
    "fastify-helmet": "^3.0.2",
    "fastify-jwt": "^1.3.1",
    "fastify-static": "^2.7.0",
    "fastify-websocket": "^1.1.2",
    "point-of-view": "^3.8.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "lint-staged": {
    "*.{js,css,md,ejs,yml,yaml}": "prettier --write"
  }
}
